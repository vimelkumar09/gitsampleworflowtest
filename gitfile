name: CI

on:
  branch
  push:
    branches: [ "main" ]

  workflow_dispatch:

jobs:
  
  build:
     on
     runs-on: ubuntu-latest

      - uses: actions/checkout@v4

      - name: Run a one-line script
        run: echo Hello, world!
      
      - name: Run a multi-line script
         run: |
          echo Add other actions to build,
          echo test, and deploy your projecttttttttttt
